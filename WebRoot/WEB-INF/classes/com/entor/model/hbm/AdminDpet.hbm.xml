<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.entor.model">
	<class name="AdminDept" table="adminDept">
		<comment>管理员部门表</comment>
		<composite-id>
		<key-many-to-one name="admin" column="admin_id"/>
		<key-many-to-one name="dept"  column="dept_id"/>
		</composite-id>		
	</class>
</hibernate-mapping>
